{
  "name": "ooga-booga-test",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "start": "tsc && node ./build/index.js",
    "start:dev": "NODE_ENV=development nodemon src/index.ts",
    "format:check": "biome format .",
    "format:write": "biome format . --write",
    "format:check:staged": "lint-staged",
    "lint": "biome lint .",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "typechecks": "tsc --noEmit",
    "prepare": "husky install"
  },
  "author": "Chef Bingbong",
  "license": "ISC",
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "chalk": "4.1.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "viem": "2.21.45",
    "winston": "^3.9.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.11",
    "husky": "7.0.0",
    "lint-staged": "^15.2.9",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
